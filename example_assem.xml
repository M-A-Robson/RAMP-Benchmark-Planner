<?xml version="1.0"?>
<assembly>
    <!-- outer horizontal beams-->
    <component beam="b1" base="True"/>
    <component beam="b4" flipped="True"/>
    <!-- vertical beams -->
    <component beam="b2" />
    <component beam="b3" />
    <!-- outer ring connections -->
    <connection name="C1">
        <element component="b1" joint="J1" />
        <element component="b2" joint="J3" />
    </connection>
    <connection name="C2">
        <element component="b1" joint="J2" />
        <element component="b3" joint="J5" />
    </connection>
    <connection name="C3">
        <element component="b2" joint="J4" />
        <element component="b4" joint="J7" />
    </connection>
    <connection name="C4">
        <element component="b3" joint="J6" />
        <element component="b4" joint="J8" />
    </connection>
</assembly>
