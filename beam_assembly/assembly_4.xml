<?xml version="1.0"?>
<assembly>
    <!-- outer horizontal beams -->
    <component beam="beam7" base="True"/>
    <component beam="beam8" flipped="True"/>
    <!-- outer vertical beams -->
    <component beam="beam4" />
    <component beam="beam5" />
    <!-- angle beam -->
    <component beam="beam6" />
    <!-- inner horizontal beam -->
    <component beam="beam9" />
    <!-- outer ring connections -->
    <connection name="C1">
        <element component="beam4" joint="J4-1" />
        <element component="beam7" joint="J7-1" />
    </connection>
    <connection name="C2">
        <element component="beam5" joint="J5-1" />
        <element component="beam7" joint="J7-5" />
    </connection>
    <connection name="C3">
        <element component="beam4" joint="J4-3" />
        <element component="beam8" joint="J8-1" />
    </connection>
    <connection name="C4">
        <element component="beam5" joint="J5-3" />
        <element component="beam8" joint="J8-5" />
    </connection>
    <!-- angle beam connections to outer ring -->
    <connection name="C5">
        <element component="beam6" joint="J6-1" />
        <element component="beam7" joint="J7-2" />
    </connection>
    <connection name="C6">
        <element component="beam6" joint="J6-3" />
        <element component="beam8" joint="J8-4" />
    </connection>
    <!--conenctions between central horizontal and vertical beams-->
    <connection name="C11">
        <element component="beam9" joint="J9-1" />
        <element component="beam4" joint="J4-2" />
    </connection>
    <connection name="C12">
        <element component="beam9" joint="J9-5" />
        <element component="beam5" joint="J5-2" />
    </connection>
    <!-- connection between inner beam-->
    <connection name="C13">
        <element component="beam9" joint="J9-3" />
        <element component="beam6" joint="J6-2" />
    </connection>
</assembly>